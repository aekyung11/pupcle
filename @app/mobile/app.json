{
  "expo": {
    "name": "My_Project_Here",
    "slug": "My_Project_Here",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "my-project-here",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "my.project.here.mobile.dev"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "native.tsx",
        "native.jsx",
        "native.ts",
        "native.js",
        "jsx",
        "js",
        "json",
        "ts",
        "tsx",
        "graphql",
        "gql"
      ]
    }
  }
}
